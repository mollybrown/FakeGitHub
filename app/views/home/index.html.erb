<div class="container">
  <div class="login-box">
    <i class="fa fa-github fa-5x" aria-hidden="true"></i>
    <h3>Sign in to FakeGitHub</h3>
    <div class="btn btn-block btn-success">
      <%= link_to "Sign In","https://github.com/login/oauth/authorize?client_id=#{ENV['GITHUB_KEY']}", class:"btn " %>
    </div>
  </div>
</div>
