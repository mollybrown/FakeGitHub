<%= render partial: 'layouts/navbar' %>

<div class="main">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h2>My Recent Activity/Commits</h2>
        <table class="table">
          <tbody>
          <% @commits.each do |commit| %>
            <tr>
              <td>
                <%= commit[:repo] %>
              </td>
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
      <div class="col-md-6">
        <h2>Activity/Commits of followed users</h2>
        <table class="table">
          <tbody>
          <% @following_commits.each do |commit| %>
            <tr>
              <td>
                <%= commit[:repo] %>
              </td>
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
